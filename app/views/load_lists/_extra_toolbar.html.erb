<%if params[:action] == "show" or params[:action] == 'process_handle'%>
  <%if can? :ship,resource%>
    <li>
      <%= link_to image_tag("auction-hammer--arrow.png",:alt => "发货处理") , process_handle_load_list_path(resource),:title => "发货处理",:class => "tipsy btn_process_handle"  %>
    </li>
  <%end%>
  <%if can? :build_act_load_list,resource%>
    <li>
      <%= link_to image_tag("act-load-list.png",:alt => "生成实际装车清单",:size => '16x16') ,  build_act_load_list_load_list_path(resource),:title => "生成实际装车清单",:class => "tipsy"  %>
    </li>
  <%end%>

  <li>
    <%= link_to image_tag("document-excel-table.png",:alt => "导出为EXCEL"), export_excel_load_list_path(resource,:format => :js),:title => "导出为EXCEL",:class => "btn_export_excel tipsy"  %>
  </li>
<%end%>

<%if params[:action] == "build_act_load_list" %>
  <li>
    <%= link_to_function image_tag("disk-black.png",:alt => "保存实际装车清单"), "$('#act_load_list_form').trigger('submit')",:title => "保存实际装车清单",:class => "tipsy btn_save"  %>
  </li>
<%end%>
