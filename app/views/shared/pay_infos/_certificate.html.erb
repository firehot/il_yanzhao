<!--客户提款凭条-->
<table class='pay_info_certificate' style='border-collapse : collapse;' >
  <thead>
    <tr style='height : 9mm;'>
      <td colspan='7' style='width : 175mm;text-align : right;'><%=resource.bill_date%></td>
    </tr>
  </thead>
  <tbody>
    <tr style='height : 9mm;'>
      <td style='width : 25mm;'>&nbsp;</td>
      <td style='width : 25mm;'>&nbsp;</td>
      <td style='width : 25mm;'><%=resource.account_name %></td>
      <td style='width : 25mm;'>&nbsp;</td>
      <td colspan='3' style='width : 75mm;'><%= resource.account_no %></td>
    </tr>
    <tr>
      <td rowspan='2' style='width : 25mm;'>&nbsp;</td>
      <td style='width : 25mm;'>&nbsp;</td>
      <td colspan='5' style='padding : 0;maring : 0;width : 125mm;'>
        <% bills = resource.carrying_bills %>
        <table style='border-collapse : collapse;padding : 0;margin : 0;'>
          <%(0..10).step(5) do |step|%>
            <tr style='height : 9mm;'>
              <%(step + 0 .. 4 + step).each do |index|%>
                <td style='width : 25mm;'>&nbsp;<%= bills[index].try(:bill_no) %></td>
              <%end%>
            </tr>
          <%end%>
        </table>
      </td>
    </tr>
    <tr style='height : 9mm;'>
      <td style="width : 25mm;">&nbsp;</td>
      <td colspan='2' style="width : 50mm;"><%=resource.carrying_bills.size %></td>
      <td style="width : 25mm;">&nbsp;</td>
      <td colspan='2' style="width : 50mm;"><%=resource.sum_act_pay_fee %></td>
    </tr>
  </tbody>
</table>
