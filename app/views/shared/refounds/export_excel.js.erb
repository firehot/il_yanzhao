<%
  #判断是显示分组还是显示普通表格
  partial = "shared/refounds/excel"
  partial = "shared/refounds/excel_group" if params[:show_group]

%>
var render_table = '<%= escape_javascript(render partial)%>'.replace(/\n\s*/g,"");
var table =$(render_table).wrap("<div />");
//table.find('td').css({border : "thin solid #000"});
$.export_excel(table.html());
